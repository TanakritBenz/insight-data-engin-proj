{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "description": "Frontend for my insight data engineering project",
    "repository": "https://github.com/TanakritBenz/insight-data-engin-proj",
    "scripts": {
        "start": "node ./bin/www",
        "monitor": "nodemon app.js",
