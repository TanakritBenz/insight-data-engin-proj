    "devDependencies": {
        "mocha": "^3.0.2",
        "nodejs-repo-tools": "git+https://git@github.com/GoogleCloudPlatform/nodejs-repo-tools.git",
        "supertest": "^2.0.0"
    },
    "engines": {
        "node": ">=4.3.2"
    }
}
